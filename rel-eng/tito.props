# vim: ts=2:sw=2:et
#
# Please keep blacklist/whitelist sorted
#
[requirements]
tito=0.6.1

[buildconfig]
builder = obal.tito_extensions.git_annex_spec_builder.GitAnnexSpecBuilder
tagger = tito.tagger.ReleaseTagger
lib_dir = rel-eng/custom/

[pulpcore-3.17-el7]
disttag = .el7
blacklist =
  ansible-lint
  python-pulp-ostree
scl = tfm-pulpcore

[pulpcore-3.17-el8]
disttag = .el8
blacklist =
  subscription-manager
  tfm-pulpcore


[katello-pulpcore-nightly-el7]
whitelist = 
        python-aiosignal
        python-beautifulsoup4
        python-deprecated
        python-frozenlist
        python-gitdb
        python-gitpython
